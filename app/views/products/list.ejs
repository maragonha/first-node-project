<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Produtos</title>
  </head>
  <body>
    <h1>Produtos</h1>

    <table>
      <thead>
        <tr>
          <th>Id</th>
          <th>Nome</th>
          <th>Descrição</th>
          <th>Preço</th>
        </tr>
      </thead>
      <tbody>
        <% for(var i=0; i<list.length; i++) { %>
          <tr>
            <td><%= list[i].title %></td>
            <td><%= list[i].id %></td>
            <td><%= list[i].description %></td>
            <td><%= list[i].price %></td>
          </tr>
        <% } %>
      </tbody>
    </table>
  </body>
</html>
